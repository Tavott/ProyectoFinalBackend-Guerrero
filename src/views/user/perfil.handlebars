<section class="perfil-container">
  {{#if error.status}}
    <p>{{error.message}}</p>
  {{/if}}
  <article class="user_info text-center">
    <h2>Perfil</h3>
    <div class="card text-start w-100" style="width: 18rem;">
      <div class="card-body">
        <h5 class="card-title mb-3">Fullname: {{user.first_name}} {{user.last_name}}</h5>
        <h6 class="card-subtitle mb-2 text-body-secondary">Email: {{user.email}}</h6>
        <h6 class="card-subtitle mb-2 text-body-secondary">Age: {{user.age}}</h6>
        <a
          href="/logout"
          class="card-link text-end"
          id="logout"
          style="display: block;"
        >Log Out</a>
      </div>
    </div>
  </article>
</section>